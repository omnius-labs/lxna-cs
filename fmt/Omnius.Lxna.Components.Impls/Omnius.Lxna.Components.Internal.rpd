syntax v1.0;

using "Omnius.Lxna.Components.Models";

namespace "Omnius.Lxna.Components.Internal";
option csharp_access_level "internal";

message ThumbnailMetadata {
    FileLength: uint64,
    FileLastWriteTime: timestamp,
}

message ThumbnailEntity {
    Metadata: ThumbnailMetadata,
    Contents: vector<ThumbnailContent>(capacity: 8192),
}
