{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ui-desktop (Win)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/tmp/bin/win/Omnius.Lxna.Ui.Desktop/Debug/net5.0/Omnius.Lxna.Ui.Desktop.dll",
            "args": [
                "--config",
                "./demo/ui-desktop/config.yml",
                "--storage",
                "./demo/ui-desktop/storage",
                "--logs",
                "./demo/ui-desktop/logs",
                "-v"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}
