{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "design-ui-desktop (win)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "design-build",
            "program": "${workspaceFolder}/tmp/bin/win-vscode-design/Omnius.Lxna.Ui.Desktop/Debug/net7.0/Omnius.Lxna.Ui.Desktop.dll",
            "cwd": "${workspaceFolder}/demo",
            "args": ["-d", "Main"],
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "design-ui-desktop (linux)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "design-build",
            "program": "${workspaceFolder}/tmp/bin/linux-vscode-design/Omnius.Lxna.Ui.Desktop/Debug/net7.0/Omnius.Lxna.Ui.Desktop.dll",
            "cwd": "${workspaceFolder}/demo",
            "args": ["-d", "Main"],
            "envFile": "${workspaceFolder}/.env",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "ui-desktop (Win)",
            "type": "coreclr",
            "request": "launch",
